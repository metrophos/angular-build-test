{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "outDir": "target/dist",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "moduleResolution": "node",
        "lib": ["es2015", "dom"]
    },
    "exclude": [
        "node_modules",
        "target"
    ],
    "compileOnSave": false,
    "buildOnSave": false
}
